{
  "groups": [
    {
      "name": "bearmq",
      "type": "com.bearmq.client.config.BearConfig",
      "description": "BearMQ configuration properties"
    }
  ],
  "properties": [
    {
      "name": "bearmq.username",
      "type": "java.lang.String",
      "description": "bearmq username"
    },
    {
      "name": "bearmq.password",
      "type": "java.lang.String",
      "description": "bearmq password"
    },
    {
      "name": "bearmq.api-key",
      "type": "java.lang.String",
      "description": "bearmq api key"
    },
    {
      "name": "bearmq.host",
      "type": "java.lang.String",
      "description": "Host address"
    },
    {
      "name": "bearmq.port",
      "type": "java.lang.Integer",
      "description": "Port",
      "defaultValue": 6667
    },
    {
      "name": "bearmq.virtual-host",
      "type": "java.lang.String",
      "description": "Virtual Host address"
    },
    {
      "name": "bearmq.listener.initial-delay-ms",
      "type": "java.lang.Integer",
      "description": "listener initial delay ms",
      "defaultValue": 500
    },
    {
      "name": "bearmq.listener.period-ms",
      "type": "java.lang.Integer",
      "description": "listener period ms",
      "defaultValue": 250
    }
  ]
}
